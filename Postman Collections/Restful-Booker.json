			"event": [
				{
					"listen": "test",
					"script": {
						"id": "57ba78e8-08a2-45d2-9a5c-e877dadf5997",
						"exec": [
							"const jsonResponse = pm.response.json();\r",
							"\r",
							"pm.test(\"Verify status code is 200\", function () \r",
							"{\r",
							"    pm.response.to.have.status(200);\r",
							"})\r",
							"\r",
							"pm.test(\"Verify token is present\", function ()\r",
							"{\r",
							"    pm.expect(jsonResponse).have.property(\"token\")\r",
							"})\r",
							"\r",
							"pm.test(\"Verify token is not empty\", function ()\r",
							"{\r",
							"    pm.expect(jsonResponse.token).to.not.be.empty;\r",
							"})\r",
							"\r",
							"pm.collectionVariables.set(\"token\", jsonResponse.token);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
